#include <stdio.h>
#include <string.h>

int main() {
    // Define two strings to be concatenated
    char str1[100] = "Hello, ";
    char str2[] = "world!";
    
    // Concatenate str2 to the end of str1
    strcat(str1, str2);
    
    // Print the concatenated string
    printf("Concatenated string: %s\n", str1);
    
    return 0;
}
